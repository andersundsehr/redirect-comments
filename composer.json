{
  "name": "andersundsehr/redirect-comments",
  "description": "Adds Comments to the redirect Module",
  "license": [
    "GPL-2.0-or-later"
  ],
  "type": "typo3-cms-extension",
  "require": {
    "php": "~8.2.0 || ~8.3.0",
    "typo3/cms-core": "^12.4",
    "typo3/cms-redirects": "^12.4"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "^7.0",
    "saschaegerer/phpstan-typo3": "^1.10.0",
    "ssch/typo3-rector": "^2.5.0"
  },
  "autoload": {
    "psr-4": {
      "AUS\\RedirectComments\\": "Classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "redirect_comments"
    }
  }
}
